{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd web-frontend/techbirdsfly-frontend-nextjs && npm run build",
  "devCommand": "cd web-frontend/techbirdsfly-frontend-nextjs && npm run dev",
  "installCommand": "cd web-frontend/techbirdsfly-frontend-nextjs && npm install",
  "framework": "nextjs",
  "outputDirectory": "web-frontend/techbirdsfly-frontend-nextjs/.next",
  "regions": ["iad1"],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,DELETE,PATCH,POST,PUT"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, Authorization"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/auth/:path*",
      "destination": "/api/auth/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/home",
      "destination": "/",
      "permanent": true
    }
  ],
  "functions": {
    "api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "crons": []
}
